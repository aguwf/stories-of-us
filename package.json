{
	"name": "stories-of-us",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build-stats": "cross-env ANALYZE=true bun run build",
		"build": "npm run db:migrate && next build",
		"check-types": "tsc --noEmit --pretty",
		"clean": "rimraf .next .swc out coverage",
		"commit": "cz",
		"check": "bunx biome check --write",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"dev": "next dev",
		"prebuild": "rimraf .next",
		"predev": "rimraf .next",
		"start": "next start"
	},
	"dependencies": {
		"@ant-design/nextjs-registry": "^1.0.0",
		"@auth/drizzle-adapter": "^1.4.2",
		"@clerk/localizations": "^2.5.4",
		"@clerk/nextjs": "^5.2.8",
		"@nextui-org/react": "^2.4.6",
		"@t3-oss/env-nextjs": "^0.10.1",
		"@tanstack/react-query": "^5.51.15",
		"@trpc/client": "^11.0.0-rc.467",
		"@trpc/react-query": "^11.0.0-rc.467",
		"@trpc/server": "^11.0.0-rc.467",
		"antd": "^5.19.3",
		"autoprefixer": "^10.4.19",
		"axios": "^1.7.3",
		"cloudinary": "^2.4.0",
		"crypto": "^1.0.1",
		"cssnano": "^7.0.4",
		"drizzle-orm": "^0.30.10",
		"embla-carousel": "^8.1.8",
		"embla-carousel-autoplay": "^8.1.8",
		"embla-carousel-react": "^8.1.8",
		"framer-motion": "^11.3.19",
		"geist": "^1.3.1",
		"hugeicons-react": "^0.3.0",
		"imagekit": "^5.0.1",
		"imagekit-javascript": "^3.0.2",
		"next": "^14.2.5",
		"next-auth": "^4.24.7",
		"next-intl": "^3.17.2",
		"next-themes": "^0.3.0",
		"postgres": "^3.4.4",
		"pino": "^9.3.2",
		"pino-pretty": "^11.2.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"server-only": "^0.0.1",
		"superjson": "^2.2.1",
		"uuid": "^10.0.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@commitlint/types": "^19.0.3",
		"@next/bundle-analyzer": "^14.2.5",
		"@types/node": "^20.14.13",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/uuid": "^10.0.0",
		"dotenv": "^16.0.3",
		"drizzle-kit": "^0.21.4",
		"eslint": "^8.57.0",
		"eslint-config-next": "^14.2.5",
		"eslint-plugin-drizzle": "^0.2.3",
		"jiti": "^1.21.6",
		"lefthook": "^1.7.12",
		"postcss": "^8.4.39",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"rimraf": "^6.0.1",
		"tailwindcss": "^3.4.7",
		"ts_dependency_graph": "^2.1.1",
		"typescript": "^5.5.4"
	},
	"ct3aMetadata": {
		"initVersion": "7.36.2"
	},
	"release": {
		"branches": ["main"],
		"plugins": [
			[
				"@semantic-release/commit-analyzer",
				{
					"preset": "conventionalcommits"
				}
			],
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			[
				"@semantic-release/npm",
				{
					"npmPublish": false
				}
			],
			"@semantic-release/git",
			"@semantic-release/github"
		]
	}
}
